    "mocha": "^2.4.5",
    "mocha-sprinkles": "^2.2.1",
    "nock": "^8.0.0",
    "publish-please": "^2.1.4",
    "rewire": "2.5.1",
    "shelljs": "^0.7.0"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "files": [
    "src",
    "index.js"
