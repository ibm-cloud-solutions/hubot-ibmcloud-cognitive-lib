{
  "name": "hubot-ibmcloud-cognitive-lib",
  "version": "0.0.26",
  "description": "hubot-ibmcloud-cognitive-lib provides cognitive libraries to be assist Hubot scripts.",
  "main": "index.js",
  "respository": {
    "url": "https://github.com/ibm-cloud-solutions/hubot-ibmcloud-cognitive-lib",
    "type": "git"
  },
  "bin": {
